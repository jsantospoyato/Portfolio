doctype html
html lang='en'
    head
        meta(charset='utf-8')
        meta(name='viewport' content='width=device-width, initial-scale=1')
    
    body
        - const classes = ['header', 'main', 'footer']

        header(class=myClasses[0])
            h1
            nav
                ul
                    li
                        a
        main(class=myClasses[1])
            section
                h2
                p
        footer(class=myClasses[1])
            section
                p